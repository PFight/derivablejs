{
  "name": "derivable",
  "version": "0.13.0-beta4",
  "description": "Functional Reactive State for JavaScript & TypeScript",
  "author": "David Sheldrick",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "test": "mocha --compilers js:babel-register",
    "stats": "node scripts/stats.js",
    "docs": "node scripts/make-docs.js",
    "clean": "rm -rf dist",
    "all": "npm run clean && npm run build && npm run test && npm run docs"
  },
  "main": "dist/derivable.js",
  "typings": "dist/derivable.d.ts",
  "typescript": {
    "definition": "dist/derivable.d.ts"
  },
  "keywords": [
    "immutable",
    "cursor",
    "typescript",
    "functional",
    "state",
    "react",
    "reactive",
    "dynamic"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "bluebird": "^2.9.34",
    "colors": "^1.1.2",
    "immutable": "^3.7.4",
    "mocha": "^2.2.5",
    "rollup": "^0.26.1",
    "source-map-support": "^0.3.2",
    "uglify-js": "^2.4.24"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ds300/derivablejs"
  },
  "bugs": {
