{
  "name": "derivable",
  "version": "0.13.0-beta7",
  "description": "Functional Reactive State for JavaScript & TypeScript",
  "author": "David Sheldrick",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "test": "./node_modules/.bin/mocha --compilers js:babel-register --require source-map-support/register --recursive",
    "coverage": "./node_modules/.bin/nyc npm test",
    "show-coverage": "npm run coverage && ./node_modules/.bin/nyc report --reporter=html && open coverage/index.html",
    "report-coverage": "npm run coverage && nyc ./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls",
    "stats": "node scripts/stats.js",
    "docs": "node scripts/make-docs.js",
    "clean": "rm -rf dist",
    "all": "npm run clean && npm run build && npm run coverage && npm run docs && npm run stats"
  },
  "main": "dist/derivable.js",
  "typings": "dist/derivable.d.ts",
  "typescript": {
    "definition": "dist/derivable.d.ts"
  },
  "keywords": [
    "immutable",
    "cursor",
    "typescript",
    "functional",
    "state",
    "react",
    "reactive",
    "dynamic"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "bluebird": "^2.9.34",
    "chai": "^3.5.0",
    "colors": "^1.1.2",
    "coveralls": "^2.11.9",
    "immutable": "^3.7.4",
    "mocha": "^2.2.5",
    "nyc": "^6.4.0",
    "rollup": "^0.26.1",
    "source-map-support": "^0.3.2",
    "uglify-js": "^2.4.24"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ds300/derivablejs"
  },
  "bugs": {
    "url": "https://github.com/ds300/derivablejs/issues"
  },
  "license": "Apache 2.0"
}
